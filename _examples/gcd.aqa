# The Euclidean Algorithm
#Â Calculates the GCD of two numbers

SUBROUTINE gcd(a, b)
    OUTPUT "gcd"
    IF a == 0 THEN
        return b
    ENDIF

    IF b == 0 THEN
        return a
    ENDIF

    return gcd(b, a mod b)
ENDSUBROUTINE

SUBROUTINE gcd2(a, b)
    OUTPUT "gcd2"
    IF a == 0 THEN
        return b
    ENDIF

    IF b == 0 THEN
        return a
    ENDIF

    return gcd2(b, a mod b)
ENDSUBROUTINE