# The Euclidean Algorithm
#Â Calculates the GCD of two numbers

SUBROUTINE mod(a, b)
    floor <- a / b
    result <-  a - (floor * b)

    return result
ENDSUBROUTINE

SUBROUTINE gcd(a, b)
    IF a == 0 THEN
        return b
    ENDIF

    IF b == 0 THEN
        return a
    ENDIF

    return gcd(b, mod(a, b))
ENDSUBROUTINE

